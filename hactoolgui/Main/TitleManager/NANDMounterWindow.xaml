<Window x:Class="HACGUI.Main.TitleManager.NANDMounterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HACGUI.Main.TitleManager"
        mc:Ignorable="d"
        Title="NAND Mounter" Height="70" Width="270">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Right" Click="MountClicked">Mount</Button>
            <ComboBox Name="ComboBox" DockPanel.Dock="Left" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
        </DockPanel>
    </Grid>
</Window>
